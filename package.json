{
  "name": "getit",
  "version": "2.1.0",
  "description": "A Chrome Extension for download things",
  "private": true,
  "scripts": {
    "build": "parcel build src/manifest.json --no-content-hash --no-source-maps --dist-dir dist --no-cache --detailed-report 0",
    "prebuild": "rm -rf ./dist",
    "watch": "parcel watch src/manifest.json --dist-dir dist --no-cache --no-hmr --no-source-maps",
    "postinstall": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valsaven/GetIt.git"
  },
  "keywords": [
    "downloader",
    "chrome",
    "javascript",
    "chrome-extension",
    "instagram",
    "twitter",
    "z0r.de"
  ],
  "author": "Val Saven <val.saven@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/valsaven/GetIt/issues"
  },
  "homepage": "https://github.com/valsaven/GetIt#readme",
  "devDependencies": {
    "@parcel/config-webextension": "2.6.0",
    "husky": "8.0.1",
    "parcel": "2.6.0"
  }
}
